{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": ".",
    "types": ["jest"],
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "experimentalDecorators": true
  }
}
